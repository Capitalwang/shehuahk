					<dd><a {CLASS_COLOR EX=$post.symmetry[1] CLASS='this'} href="javascript:;" onclick="sbon(this , 'EX')">EX</a><input type="hidden" name="symmetry[]" value="{$post.symmetry[1]}" /></dd><dt></dt>
