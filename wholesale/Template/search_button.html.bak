					<dd class="t"><a {CLASS_COLOR Blue=$post.Fent_color[0] CLASS='this'} href="javascript:;" onclick="sbon(this , 'Blue')">Blue</a><input type="hidden" name="Fent_color[]" value="{$post.Fent_color[0]}" /></dd><dt></dt>
					<dd><a {CLASS_COLOR Yellow=$post.Fent_color[1] CLASS='this'} href="javascript:;" onclick="sbon(this , 'Yellow')">Yellow</a><input type="hidden" name="Fent_color[]" value="{$post.Fent_color[1]}" /></dd><dt></dt>
					<dd><a {CLASS_COLOR Green=$post.Fent_color[2] CLASS='this'} href="javascript:;" onclick="sbon(this , 'Green')">Green</a><input type="hidden" name="Fent_color[]" value="{$post.Fent_color[2]}" /></dd><dt></dt>
					<dd><a {CLASS_COLOR Red=$post.Fent_color[3] CLASS='this'} href="javascript:;" onclick="sbon(this , 'Red')">Red</a><input type="hidden" name="Fent_color[]" value="{$post.Fent_color[3]}" /></dd><dt></dt>
					<dd><a {CLASS_COLOR Orange=$post.Fent_color[4] CLASS='this'} href="javascript:;" onclick="sbon(this , 'Orange')">Orange</a><input type="hidden" name="Fent_color[]" value="{$post.Fent_color[4]}" /></dd><dt></dt>
					<dd class="w"><a {CLASS_COLOR White=$post.Fent_color[5] CLASS='this'} href="javascript:;" onclick="sbon(this , 'White')">White</a><input type="hidden" name="Fent_color[]" value="{$post.Fent_color[5]}" /></dd>
				</dl>
