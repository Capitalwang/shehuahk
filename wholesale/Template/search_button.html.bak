			<div class="bon cow">
				<strong>钻重</strong>
				<span>
					<input type="text" name="weight[]" value="{$post.weight[0]}" onfocus="sprintf('%.2f' , this , true)" /> ct ~ 
					<input type="text" name="weight[]" value="{$post.weight[1]}" onfocus="sprintf('%.2f' , this , true)" /> ct
				</span>
				<div><input type="submit" class="st" value="查 询" /></div>
			</div>
		</form>
		{if $get.go}
		<form action="{write_url open='cart' action='add' DEL='ALL'}" method="post">
			<div class="Page">
				<div>为您筛选出<b>{$list_page.Record}</b>颗钻石等您选择</div>
				{if $list_text}
				<ul>
					<li><a href="{write_url url=$thisurl page=$list_page.up}">上一页</a></li>
					{foreach item=Npc from=$list_page.PageCode}<li><a href="{write_url url=$thisurl page=$Npc}" {if $Npc == $get.page}class="this"{/if}>{$Npc}</a></li>{/foreach}
					<li><a href="{write_url url=$thisurl page=$list_page.down}">下一页</a></li>
				</ul>
				{/if}
				<i class="clear"></i>
			</div>
		
