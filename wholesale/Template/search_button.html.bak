					<dd><a {CLASS_COLOR K=$post.color[7] CLASS='this'} href="javascript:;" onclick="sbon(this , 'K')">K</a><input type="hidden" name="color[]" value="{$post.color[7]}" /></dd><dt></dt>
					<dd><a {CLASS_COLOR L=$post.color[8] CLASS='this'} href="javascript:;" onclick="sbon(this , 'L')">L</a><input type="hidden" name="color[]" value="{$post.color[8]}" /></dd><dt></dt>
					<dd><a {CLASS_COLOR M=$post.color[9] CLASS='this'} href="javascript:;" onclick="sbon(this , 'M')">M</a><input type="hidden" name="color[]" value="{$post.color[9]}" /></dd><dt></dt>
					<dd class="w"><a {CLASS_COLOR N=$post.color[10] CLASS='this'} href="javascript:;" onclick="sbon(this , 'N')">N</a><input type="hidden" name="color[]" value="{$post.color[10]}" /></dd>
				</dl>
				<strong>抛光</strong>
				<dl class="v2">
					<dd class="t"><a {CLASS_COLOR ID=$post.buffing[0] CLASS='this'} href="javascript:;" onclick="sbon(this , 'ID')">ID</a><input type="hidden" name="buffing[]" value="{$post.buffing[0]}" /></dd><dt></dt>
					<dd><a {CLASS_COLOR EX=$post.buffing[1] CLASS='this'} href="javascript:;" onclick="sbon(this , 'EX')">EX</a><input type="hidden" name="buffing[]" value="{$post.buffing[1]}" /></dd><dt></dt>
