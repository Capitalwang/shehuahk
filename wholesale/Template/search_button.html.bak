					<dd><a {CLASS_COLOR S=$post.Fent_Isity[3] CLASS='this'} href="javascript:;" onclick="sbon(this , 'S')">S</a><input type="hidden" name="Fent_Isity[]" value="{$post.Fent_Isity[3]}" /></dd><dt></dt>
