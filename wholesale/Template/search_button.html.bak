				<dl class="v2">
					<dd class="t"><a {CLASS_COLOR N=$post.Fent_Isity[0] CLASS='this'} href="javascript:;" onclick="sbon(this , 'N')">N</a><input type="hidden" name="Fent_Isity[]" value="{$post.Fent_Isity[0]}" /></dd><dt></dt>
					<dd><a {if $post.Fent_Isity[1]=='F/SL'}class='this'{/if} href="javascript:;" onclick="sbon(this , 'F/SL')">F</a><input type="hidden" name="Fent_Isity[]" value="{$post.Fent_Isity[1]}" /></dd><dt></dt>
